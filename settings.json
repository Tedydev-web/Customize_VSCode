{
  // Editor Settings
  "editor.fontFamily": "Dank Mono, Arial, Helvetica, sans-serif",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.cursorWidth": 1,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.lineHeight": 30,
  "editor.letterSpacing": 0.5,
  "editor.tabSize": 6,
  "editor.minimap.renderCharacters": false,
  "editor.suggestSelection": "first",
  "editor.inlineSuggest.enabled": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "none",
  "editor.matchBrackets": "never", // Specific to HTML and CSS
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": false,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 200,
  "editor.linkedEditing": true,
  "editor.semanticHighlighting.enabled": true,
  "editor.inlayHints.enabled": "on",

  // File Settings
  "files.trimTrailingWhitespace": true,
  "files.autoSaveDelay": 5000,
  "files.exclude": {
    "**/.history": true,
    "**/.next": true,
    "**/.nuxt": true
  },

  // Explorer Settings
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,

  // Terminal Settings
  "terminal.integrated.lineHeight": 1.3,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.letterSpacing": 0.5,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.env.osx": {},

  // GitLens Settings
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true,
    "suppressLineUncommittedWarning": true
  },
  "gitlens.codeLens.enabled": false,

  // Language Specific Settings
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.matchBrackets": "never",
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[css]": {
    "editor.matchBrackets": "never"
  },
  "[markdown]": {
    "editor.wordWrap": "bounded"
  },

  // Emmet Settings
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.triggerExpansionOnTab": true,

  // JavaScript/TypeScript Settings
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",

  // HTML/CSS/SCSS Settings
  "html.format.wrapLineLength": 70,
  "scss.lint.unknownAtRules": "ignore",
  "scss.lint.unknownProperties": "ignore",
  "css.lint.unknownAtRules": "ignore",
  "css.lint.unknownProperties": "ignore",

  // Tailwind CSS Settings
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass",
    ".*Class",
    ".*ClassName"
  ],

  // Miscellaneous Settings
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editor.centeredLayoutFixedWidth": true,
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Evondev Dracula Author",
  "workbench.fontAliasing": "antialiased",
  "window.zoomLevel": 1.75,
  "window.newWindowDimensions": "maximized",

  // Live Server Settings
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,

  // ESLint Settings
  "eslint.alwaysShowStatus": true,

  // Glean Settings
  "glean.showConversionWarning": false,
  "glean.useExportDefault": true,

  // Turbo Console Log Settings
  "turboConsoleLog.logMessagePrefix": "",
  "turboConsoleLog.addSemicolonInTheEnd": true,
  "turboConsoleLog.includeFileNameAndLineNum": false,

  // CodeSnap Settings
  "codesnap.backgroundColor": "transparent",

  // Color Highlight Settings
  "color-highlight.markerType": "underline",

  // VS IntelliCode Settings
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",

  // Total TypeScript Settings
  "totalTypeScript.hideAllTips": true,

  // Indent Rainbow Settings
  "indentRainbow.colors": [
    "rgba(38, 70, 83,0.2)",
    "rgba(42, 157, 143,0.2)",
    "rgba(233, 196, 106, 0.2)",
    "rgba(244, 162, 97,0.2)",
    "rgba(231, 111, 81,0.2)",
    "rgba(231, 111, 81,0.15)",
    "rgba(231, 111, 81,0.1)",
    "rgba(231, 111, 81,0.005)"
  ],
  "indentRainbow.tabmixColor": "rgba(128,32,96,0)",
  "indentRainbow.errorColor": "rgba(128,32,32,0)",

  // Console Ninja Settings
  "console-ninja.featureSet": "Pro",

  // Custom Colors
  "workbench.colorCustomizations": {
    "editor.linkedEditingBackground": "#ffffff00",
    "selection.background": "#828bb828"
  },

  // Git Settings
  "git.openRepositoryInParentFolders": "never",

  // Custom CSS on VS Code
  "vscode_custom_css.imports": [
    "file:///Users/tedydev/Workhome/GitHub/VSCode/color.css",
    "file:///D:/OneDrive-qph6/Workplace/Dev/VSCode/color.css"
  ],

  // Extensions Settings
  "tabnine.experimentalAutoImports": true,

  // Power Mode Settings
  "powermode.combo.counterSize": 10,
  "powermode.combo.location": "off",
  "powermode.combo.threshold": 3,
  "powermode.combo.timeout": 0,
  "powermode.combo.timerEnabled": "hide",
  "powermode.explosions.offset": 0.2,
  "powermode.explosions.size": 3.5,
  "powermode.shake.enabled": false,
  "powermode.shake.intensity": 5,
  "powermode.enabled": true,

  // Better Comments Settings
  "better-comments.multilineComments": true,
  "better-comments.highlightPlainText": false,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],

